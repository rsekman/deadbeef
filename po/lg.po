#: ../main.c:129
#, c-format
msgid "Usage: deadbeef [options] [--] [file(s)]\n"
msgstr ""

#: ../main.c:130
#, c-format
msgid "Options:\n"
msgstr "Obuwayiro:\n"

#: ../main.c:131
#, c-format
msgid "   --help  or  -h     Print help (this message) and exit\n"
msgstr "   --help  oba  -h     Bw'ossawo kamu ku buno, okoma ku kufuna bino by'osoma\n"

#: ../main.c:132
#, c-format
msgid "   --quit             Quit player\n"
msgstr "   --quit             Kano kaleetera puloguramu okumala\n"

#: ../main.c:133
#, c-format
msgid "   --version          Print version info and exit\n"
msgstr "   --version          Kano kaleetera puloguramu okukoma ku kukulaga olwandika lwa yo\n"

#: ../main.c:134
#, c-format
msgid "   --play             Start playback\n"
msgstr "   --play             Kano kaleetera pulguramu okutandika enyimba\n"

#: ../main.c:135
#, c-format
msgid "   --stop             Stop playback\n"
msgstr "   --stop             Kano kaleetera pulguramu okukomya oluyimba\n"

#: ../main.c:136
#, c-format
msgid "   --pause            Pause playback\n"
msgstr "   --pause            Kano kaleetera puloguramu okuyimirizamu oluyimba\n"

#: ../main.c:137
#, c-format
msgid "   --toggle-pause     Toggle pause\n"
msgstr ""

#: ../main.c:138
#, c-format
msgid ""
"   --play-pause       Start playback if stopped, toggle pause otherwise\n"
msgstr ""

#: ../main.c:139
#, c-format
msgid "   --next             Next song in playlist\n"
msgstr "   --next             Kano kaleetera puloguramu okutandika oluyimba oluddako mu lukalala\n"

#: ../main.c:140
#, c-format
msgid "   --prev             Previous song in playlist\n"
msgstr "   --prev             Kano kaleetera puloguramu okutandika olukulembera lw'ebadde ko mu lukalala\n"

#: ../main.c:141
#, c-format
msgid "   --random           Random song in playlist\n"
msgstr "   --random           Kano kaleetera puloguramu okuyita mu nyimba muwawa\n"

#: ../main.c:142
#, c-format
msgid "   --queue            Append file(s) to existing playlist\n"
msgstr "   --queue            Kano kaleetera puloguramu okwongera nyimba ku lukalala\n"

#: ../main.c:143
#, c-format
msgid "   --gui PLUGIN       Tells which GUI plugin to use, default is \"GTK2\"\n"
msgstr ""

#: ../main.c:144
#, c-format
msgid "   --nowplaying FMT   Print formatted track name to stdout\n"
msgstr "   --nowplaying FMT  Kano kaleetera puloguramu okuwandikawo erinnya ly'olutambi\n"

#: ../main.c:145
#, c-format
msgid ""
"                      FMT %%-syntax: [a]rtist, [t]itle, al[b]um,\n"
"                      [l]ength, track[n]umber, [y]ear, [c]omment,\n"
"                      copy[r]ight, [e]lapsed\n"
msgstr "                      FMT %%-empandika: a: muyimbi, t: Linnya lya luyimba, b: Alubamu,\n                      : Buwanvu bwa luyimba,n: Lutambi,y: Mwaka mwe lwafulumira, c: Ebinnyonyola,\n                      r: Ebikugizo by'obwa nannyini, e: Ekitundu ky'oluyimba ekiwulirizidwako\n"

#: ../main.c:148
#, c-format
msgid ""
"                      example: --nowplaying \"%%a - %%t\" should print "
"\"artist - title\"\n"
msgstr ""

#: ../main.c:149
#, c-format
msgid "                      for more info, see %s\n"
msgstr ""

#: ../main.c:150
#, c-format
msgid "                      NOTE: --nowplaying is deprecated.\n"
msgstr ""

#: ../main.c:151
#, c-format
msgid ""
"   --nowplaying-tf FMT  Print formatted track name to stdout, using the new "
"title formatting\n"
msgstr ""

#: ../main.c:152
#, c-format
msgid ""
"                      FMT syntax: http://github.com/DeaDBeeF-"
"Player/deadbeef/wiki/Title-formatting-2.0\n"
msgstr ""

#: ../main.c:153
#, c-format
msgid ""
"                      example: --nowplaying-tf \"%%artist%% - %%title%%\" "
"should print \"artist - title\"\n"
msgstr ""

#: ../main.c:154
#, c-format
msgid "   --volume [NUM]     Print or set deadbeef volume level.\n"
msgstr ""

#: ../main.c:155
#, c-format
msgid ""
"                      The NUM parameter can be specified in percents "
"(absolute value or increment/decrement)\n"
msgstr ""

#: ../main.c:156
#, c-format
msgid "                      or in dB [-50, 0] (if with suffix).\n"
msgstr ""

#: ../main.c:157
#, c-format
msgid ""
"                      Examples: --volume 80, --volume +10, --volume -5 or "
"--volume -20dB\n"
msgstr ""

#: ../main.c:158
#, c-format
msgid ""
"   --plugin=[PLUG]    Send commands to a specific plugin. Use PLUG=main to "
"send commands to deadbeef itself.\n"
msgstr ""

#: ../main.c:159
#, c-format
msgid ""
"                      To get plugin specific commands use --plugin=[PLUG] "
"--help\n"
msgstr ""

#: ../main.c:160
#, c-format
msgid ""
"   --plugin-list      List all available plugins including indication for "
"plugins that support commands.\n"
msgstr ""

