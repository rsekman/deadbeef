#: ../main.c:129
#, c-format
msgid "Usage: deadbeef [options] [--] [file(s)]\n"
msgstr "Utilizare: deadbeef [opțiuni] [--] [fișier(e)]\n"

#: ../main.c:130
#, c-format
msgid "Options:\n"
msgstr "Opțiuni:\n"

#: ../main.c:131
#, c-format
msgid "   --help  or  -h     Print help (this message) and exit\n"
msgstr "   --help  sau -h     Arată acest mesaj de ajutor și ieși\n"

#: ../main.c:132
#, c-format
msgid "   --quit             Quit player\n"
msgstr "   --quit             Ieșire din lector\n"

#: ../main.c:133
#, c-format
msgid "   --version          Print version info and exit\n"
msgstr "   --version          Arată detaliile versiunii și ieși\n"

#: ../main.c:134
#, c-format
msgid "   --play             Start playback\n"
msgstr "   --play             Pornește redarea\n"

#: ../main.c:135
#, c-format
msgid "   --stop             Stop playback\n"
msgstr "   --stop             Oprește redarea\n"

#: ../main.c:136
#, c-format
msgid "   --pause            Pause playback\n"
msgstr "   --pause            Pune pauză redării\n"

#: ../main.c:137
#, c-format
msgid "   --toggle-pause     Toggle pause\n"
msgstr "   --toggle-pause     Comută starea de pauză\n"

#: ../main.c:138
#, c-format
msgid ""
"   --play-pause       Start playback if stopped, toggle pause otherwise\n"
msgstr "   --play-pause       Pornește redarea dacă e cazul sau comută starea de pauză\n"

#: ../main.c:139
#, c-format
msgid "   --next             Next song in playlist\n"
msgstr "   --next             Piesa următoare în lista de redare\n"

#: ../main.c:140
#, c-format
msgid "   --prev             Previous song in playlist\n"
msgstr "   --prev             Piesa precedentă în lista de redare\n"

#: ../main.c:141
#, c-format
msgid "   --random           Random song in playlist\n"
msgstr "   --random           Piesă aleatoare din lista de redare\n"

#: ../main.c:142
#, c-format
msgid "   --queue            Append file(s) to existing playlist\n"
msgstr "   --queue            Adaugă fișiere la lista de redare existentă\n"

#: ../main.c:143
#, c-format
msgid "   --gui PLUGIN       Tells which GUI plugin to use, default is \"GTK2\"\n"
msgstr "   --gui MODUL        Precizează ce modul GUI să fie utilizat, implicit \"GTK2\"\n"

#: ../main.c:144
#, c-format
msgid "   --nowplaying FMT   Print formatted track name to stdout\n"
msgstr "   --nowplaying FMT   Arată în ieșirea standard numele formatat al piesei\n"

#: ../main.c:145
#, c-format
msgid ""
"                      FMT %%-syntax: [a]rtist, [t]itle, al[b]um,\n"
"                      [l]ength, track[n]umber, [y]ear, [c]omment,\n"
"                      copy[r]ight, [e]lapsed\n"
msgstr "                      Sintaxă FMT prefixată cu %%: [a]rtist, [t]itlu, al[b]um,\n                      [l]ungime, [n]umăr piesă, [y] pentru an, [c]omentariu,\n                      d[r]epturi de autor, durată r[e]dată\n"

#: ../main.c:148
#, c-format
msgid ""
"                      example: --nowplaying \"%%a - %%t\" should print "
"\"artist - title\"\n"
msgstr "exemplu: --redareacum \"%%a - %%t\" ar trebui sa arate \"artist - titlu\"\n"

#: ../main.c:149
#, c-format
msgid "                      for more info, see %s\n"
msgstr "                      pentru mai multe detalii, consultați %s\n"

#: ../main.c:150
#, c-format
msgid "                      NOTE: --nowplaying is deprecated.\n"
msgstr "NOTA: --redareacum nu mai este folosit.\n"

#: ../main.c:151
#, c-format
msgid ""
"   --nowplaying-tf FMT  Print formatted track name to stdout, using the new "
"title formatting\n"
msgstr "--redareacum-tf FMT Afiseaza numele piesei formatate catre stdout, folosind noua formatare de titlu\n"

#: ../main.c:152
#, c-format
msgid ""
"                      FMT syntax: http://github.com/DeaDBeeF-"
"Player/deadbeef/wiki/Title-formatting-2.0\n"
msgstr ""

#: ../main.c:153
#, c-format
msgid ""
"                      example: --nowplaying-tf \"%%artist%% - %%title%%\" "
"should print \"artist - title\"\n"
msgstr "exemplu: --redareacum-tf \"%%artist%% - %%titlu%%\" ar trebui sa arate \"artist - titlu\"\n"

#: ../main.c:154
#, c-format
msgid "   --volume [NUM]     Print or set deadbeef volume level.\n"
msgstr ""

#: ../main.c:155
#, c-format
msgid ""
"                      The NUM parameter can be specified in percents "
"(absolute value or increment/decrement)\n"
msgstr ""

#: ../main.c:156
#, c-format
msgid "                      or in dB [-50, 0] (if with suffix).\n"
msgstr ""

#: ../main.c:157
#, c-format
msgid ""
"                      Examples: --volume 80, --volume +10, --volume -5 or "
"--volume -20dB\n"
msgstr ""

#: ../main.c:158
#, c-format
msgid ""
"   --plugin=[PLUG]    Send commands to a specific plugin. Use PLUG=main to "
"send commands to deadbeef itself.\n"
msgstr ""

#: ../main.c:159
#, c-format
msgid ""
"                      To get plugin specific commands use --plugin=[PLUG] "
"--help\n"
msgstr ""

#: ../main.c:160
#, c-format
msgid ""
"   --plugin-list      List all available plugins including indication for "
"plugins that support commands.\n"
msgstr ""

